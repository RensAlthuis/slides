{
    "version": "0.2.0",
    "configurations": [
        {
            "type": "node",
            "request": "launch",
            "name": "Electron Main",
            "program": "${workspaceRoot}/main.js",
            "cwd": "${workspaceRoot}",
            "preLaunchTask": null,
            "env": {},
            "windows": {
                "runtimeExecutable": "${workspaceRoot}/node_modules/.bin/electron.cmd",
            },
            "runtimeExecutable": "${workspaceRoot}/node_modules/.bin/electron",
            "stopOnEntry": false,
            "runtimeArgs": [
                "."
            ],
            "console": "internalConsole",
        }
    ]
}